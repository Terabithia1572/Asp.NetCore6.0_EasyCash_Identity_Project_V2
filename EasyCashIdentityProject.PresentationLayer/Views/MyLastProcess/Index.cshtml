@model List<CustomerAccountProccess>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/CustomerLayout/Index.cshtml";
}

<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Son İşlemlerim</h3>
            </div>
            <div class="clearfix"></div>

            <div class="">
                <div class="col-md-6 col-sm-6 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Türk Lirası İle Yaptığım Son İşlemlerim</h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a href="#">Ayarlar 1</a>
                                        </li>
                                        <li>
                                            <a href="#">Ayarlar 2</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="close-link"><i class="fa fa-close"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <ul class="list-unstyled timeline">
                                @foreach (var item in Model)
                                {
                                    <li>
                                        <div class="block">
                                            <div class="tags">
                                                <a href="" class="tag">
                                                    <span>Son İşlemler</span>
                                                </a>
                                            </div>
                                            <div class="block_content">
                                                <h2 class="title">
                                                    <a>@item.ProcessType</a>
                                                </h2>
                                                <div class="byline">
                                                    <span>@item.ProcessDate.ToShortDateString()</span> by <a>Yunus İNAN</a>
                                                </div>
                                                <p class="excerpt">
                                                   @item.Description <a>Oku&nbsp;Daha Fazla</a>
                                                </p>
                                            </div>
                                        </div>
                                    </li>
                                }
                                

                            </ul>

                        </div>
                    </div>
                </div>
            </div>
            
        </div>
    <div class="clearfix"></div>
</div>
